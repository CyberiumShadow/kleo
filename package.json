{
  "name": "kleo",
  "description": "Manage your klasa bot with ease!",
  "version": "0.0.0-development",
  "author": "Charalampos Fanoulis <charalampos.fanoulis@gmail.com> (https://github.com/cfanoulis)",
  "maintainers": [
    "Charalampos Fanoulis <charalampos.fanoulis@gmail.com> (https://github.com/cfanoulis)",
    "Nejc Drobnič (https://github.com/QuantumlyTangled)"
  ],
  "repository": "cfanoulis/kleo",
  "homepage": "https://github.com/cfanoulis/kleo",
  "bugs": "https://github.com/cfanoulis/kleo/issues",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "bin": {
    "kl": "./bin/run"
  },
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "ansi-colors": "^4.1.1",
    "enquirer": "^2.3.5",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@jest/globals": "^26.0.1",
    "@oclif/dev-cli": "^1",
    "@semantic-release/git": "^9.0.0",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.5",
    "@typescript-eslint/eslint-plugin": "^3.0.1",
    "@typescript-eslint/parser": "^3.0.1",
    "eslint": "^7.1.0",
    "eslint-config-xo": "^0.29.1",
    "eslint-config-xo-typescript": "^0.29.0",
    "globby": "^11.0.0",
    "jest": "^26.0.1",
    "jest-circus": "^26.0.1",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.0.8",
    "ts-jest": "^26.0.0",
    "ts-node": "^8",
    "typescript": "^3.9.3"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "keywords": [
    "oclif",
    "klasa",
    "discord",
    "discord-bot",
    "bot",
    "cli",
    "bot-framework"
  ],
  "oclif": {
    "commands": "./lib/commands",
    "bin": "kl",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "lint": "eslint src --ext ts --fix",
    "build": "tsc",
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "rimraf lib && yarn build && oclif-dev manifest && oclif-dev readme",
    "version": "oclif-dev readme && git add README.md",
    "semantic-release": "semantic-release"
  }
}
